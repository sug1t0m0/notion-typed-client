{
  "name": "@sug1t0m0/notion-typed-client",
  "version": "6.0.0",
  "description": "Type-safe wrapper for Notion SDK with database schema injection",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "notion-typed-client": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:dev": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "vitest run --config e2e/vitest.config.e2e.ts",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "lint:unsafe": "biome lint --write --unsafe .",
    "format": "biome format --write .",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "check:unsafe": "biome check --write --unsafe .",
    "ci": "npm run generate:e2e && npm run typecheck && npm run typecheck:e2e && npm run check && npm run test && npm run build && npm run audit",
    "audit": "pnpm audit",
    "typecheck": "tsc --noEmit",
    "prepare:e2e": "npx ts-node e2e/scripts/prepareE2ETypes.ts",
    "clean:e2e": "rm -rf e2e/generated e2e/e2e.notion-typed.config.ts",
    "generate:e2e": "npm run clean:e2e && npm run prepare:e2e",
    "typecheck:e2e": "tsc --noEmit -p e2e/tsconfig.json",
    "prepublishOnly": "npm run build",
    "release:patch": "npm version patch --no-git-tag-version",
    "release:minor": "npm version minor --no-git-tag-version",
    "release:major": "npm version major --no-git-tag-version",
    "version": "npm run build",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "notion",
    "sdk",
    "typescript",
    "wrapper",
    "type-safe"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sug1t0m0/notion-typed-client.git"
  },
  "bugs": {
    "url": "https://github.com/sug1t0m0/notion-typed-client/issues"
  },
  "homepage": "https://github.com/sug1t0m0/notion-typed-client#readme",
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@10.14.0",
  "dependencies": {
    "@notionhq/client": "^4.0.2",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "chalk": "^5.5.0",
    "commander": "^14.0.0",
    "jiti": "^2.5.1",
    "json-schema-to-typescript": "^15.0.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.1",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@vitest/ui": "^3.2.4",
    "dotenv": "^17.2.1",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.33.0",
    "happy-dom": "^18.0.1",
    "jest": "^30.0.5",
    "ts-jest": "^29.4.1",
    "vite": "^7.1.5",
    "vitest": "^3.2.4"
  }
}
